getCorr <- function(row){
corr <- strsplit(row, ",")
corr <- corr[[1]]
corr <- strsplit(corr[4],"=")
corr <- corr[[1]]
corr <- strsplit(corr[2],"\\)")
corr <- corr[[1]]
return(corr)
}
